    "jade": "~1.11.0",
    "morgan": "~1.8.1",
    "nodemon": "^1.11.0",
    "sequelize": "^4.4.9",
    "sequelize-cli": "^2.8.0",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-dollar-sign": "^1.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
